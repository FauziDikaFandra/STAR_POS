Private Sub cmdangka_Click()
    frmNum.Caption = "NUMBER - VIEW"
    frmNum.Show 1
End Sub

Private Sub CmdCancel_Click()
    Unload Me
End Sub

Private Sub CmdOk_Click()
    If Right(frmSales.txtkode, 1) = "*" Then
        frmSales.txtkode = frmSales.txtkode + Grid1.Columns(0).Text
    Else
        frmSales.txtkode = Grid1.Columns(0).Text
    End If
    Unload Me
End Sub

Private Sub Grid1_KeyDown(KeyCode As Integer, Shift As Integer)
    Select Case KeyCode
    Case 13
        Call CmdOk_Click
    Case 27
        txtkode.SetFocus
    End Select
End Sub

Private Sub txtkode_KeyDown(KeyCode As Integer, Shift As Integer)
    Select Case KeyCode
    Case 13
        If txtkode = "" Then Exit Sub
        AdoLocal.ConnectionString = StrConLoc
        
        AdoLocal.RecordSource = "select top 200 plu, Description, current_price from item_master where PLU like '%" & txtkode & "%'" & _
                                "and description <> 'TIDAK AKTIF'"
        AdoLocal.Refresh
        Me.Caption = "VIEW ARTICLE - " & AdoLocal.Recordset.RecordCount
        Grid1.SetFocus
    Case 27
        Unload Me
    End Select
End Sub
